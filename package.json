{
    "name": "recal-sdk",
    "version": "0.2.2",
    "description": "Recal SDK",
    "author": "Recal <team@recal.dev>",
    "contributors": ["tkoehlerlg", "jschwxrz"],
    "license": "MIT",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "type": "module",
    "files": ["dist"],
    "scripts": {
        "build": "tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json",
        "test": "bun test tests",
        "typecheck": "tsc --noEmit",
        "format": "biome format --fix",
        "lint": "biome lint",
        "lint:fix": "biome lint --fix",
        "lint:fix:all": "biome lint --fix --unsafe",
        "check": "biome check",
        "check:fix": "biome check --fix",
        "prepack": "bun run lint && bun run typecheck && bun run test",
        "prepublishOnly": "NODE_ENV=production bun run build",
        "postpublish": "echo 'Successfully published recal-sdk@'$npm_package_version"
    },
    "keywords": ["recal", "calendar"],
    "devDependencies": {
        "@biomejs/biome": "2.1.2",
        "@types/bun": "latest",
        "tsc-alias": "^1.8.16",
        "typescript": "^5.8.3"
    },
    "dependencies": {
        "@sinclair/typebox": "^0.34.38"
    }
}
